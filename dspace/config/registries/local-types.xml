<?xml version="1.0"?>
<!--
  - local-types.xml
  -
  - An empty registry for instance-specific, local metadata. Any non-standard
  - metadata fields should be added here to avoid polluting the standard
  - namespaces.
  - 
  -->
<dspace-dc-types>

    <dspace-header>
        <title>Local metadata</title>
        <contributor.author>Andrea Bollini</contributor.author>
        <contributor.author>Luigi Andrea Pascarelli</contributor.author>
        <contributor.author>Riccardo Fazio</contributor.author>
      </dspace-header>

  <dc-schema>
    <name>local</name>
    <namespace>http://dspace.org/namespace/local/</namespace>
  </dc-schema>

<!--
  <dc-type>
    <schema>local</schema>
    <element></element>
    <qualifier></qualifier>
    <scope_note>Description of example metadata field.</scope_note>
  </dc-type>
-->

  <dc-type>
    <schema>local</schema>
    <element>message</element>
    <qualifier>claim</qualifier>
    <scope_note>Enable the user to add notes about his claiming request</scope_note>
  </dc-type>

  <dc-type>
    <schema>local</schema>
    <element>type</element>
    <qualifier>version</qualifier>
    <scope_note>Version of the item in relation to a publication at a publisher. May be one of the following values:
draft
submittedVersion
acceptedVersion
publishedVersion
updatedVersion</scope_note>
  </dc-type>

  <dc-type>
    <schema>local</schema>
    <element>publisher</element>
    <qualifier>draft</qualifier>
    <scope_note></scope_note>
  </dc-type>

  <dc-type>
    <schema>local</schema>
    <element>publisher</element>
    <qualifier>submittedVersion</qualifier>
    <scope_note></scope_note>
  </dc-type>

  <dc-type>
    <schema>local</schema>
    <element>publisher</element>
    <qualifier>acceptedVersion</qualifier>
    <scope_note></scope_note>
  </dc-type>

  <dc-type>
    <schema>local</schema>
    <element>publisher</element>
    <qualifier>publishedVersion</qualifier>
    <scope_note></scope_note>
  </dc-type>

  <dc-type>
    <schema>local</schema>
    <element>publisher</element>
    <qualifier>updatedVersion</qualifier>
    <scope_note></scope_note>
  </dc-type>

</dspace-dc-types>
